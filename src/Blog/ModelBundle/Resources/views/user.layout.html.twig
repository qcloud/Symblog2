<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{% endblock %}</title>
    {% block stylesheets %}
        <!-- Bootstrap Core CSS -->
        <link href="{{ asset('bundles/model/css/bootstrap.min.css') }}" rel="stylesheet">

        <!-- MetisMenu CSS -->
        <link href="{{ asset('bundles/model/css/plugins/metisMenu/metisMenu.min.css') }}" rel="stylesheet">

        <!-- Timeline CSS -->
        <link href="{{ asset('bundles/model/css/plugins/timeline.css') }}" rel="stylesheet">

        <!-- Custom CSS -->
        <link href="{{ asset('bundles/model/css/sb-model-2.css') }}" rel="stylesheet">

        <!-- Morris Charts CSS -->
        <link href="{{ asset('bundles/model/css/plugins/morris.css') }}" rel="stylesheet">

        <!-- Custom Fonts -->
        <link href="font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    {% endblock %}
</head>
<body>
{% block container %}
{% endblock container %}
{% block javascripts_footer %}
    <!-- jQuery Version 1.11.0 -->

    <script src="{{ asset('bundles/model/js/jquery-1.11.0.js') }}"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="{{ asset('bundles/model/js/bootstrap.min.js') }}"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="{{ asset('bundles/model/js/plugins/metisMenu/metisMenu.min.js') }}"></script>

    <!-- Morris Charts JavaScript -->
    <script src="{{ asset('bundles/model/js/plugins/morris/raphael.min.js') }}"></script>
    <script src="{{ asset('bundles/model/js/plugins/morris/morris.min.js') }}"></script>
    <script src="{{ asset('bundles/model/js/plugins/morris/morris-data.js') }}"></script>

    <!-- Custom Theme JavaScript -->
    <script src="{{ asset('bundles/model/js/sb-model-2.js') }}"></script>
{% endblock %}
</body>
</html>